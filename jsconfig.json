{
  "include": ["src/**/*"],
  "compilerOptions": {
    "target": "ES6",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src/",
    "checkJs": false,
    "jsx": "react",
    "paths": {
      "services/*": ["shared/services/*"],
      "browser-services/*": ["browser/services/*"],
      "shared/*": ["shared/*"],
      "browser-components/*": ["browser/components/*"],
      "browser-hooks/*": ["browser/hooks/*"],
      "browser/*": ["browser/*"],
      "browser-styles/*": ["browser/styles/*"]
    }
  }
}
